<h2 class=".page-header"><%= director.name %></h2>
<a href='/directors/<%= director.id %>/edit'>Edit</a>
 <%- include('../partials/deleteForm', {
        url: `/directors/${director.id}` 
    }) %>
    <% if(movieByDirector.length > 0) { %>
        <h2>Movies By Director</h2>
        <% movieByDirector.forEach(movie => { %>
            <a href="/movies/<%= movie.id %>">
        <img height='150' width='100' src="<%= movie.coverImagePath %>">
        </a>
    <% }) %> 
    <% } %>
